<div class="container">
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
      <h3><%= @project.name %></h3>
      <div class="btn-group">
        <%= link_to 'Upload Units', new_dataset_path(@project.id), class: 'btn btn-primary' %>
        <%= link_to 'Generate Report', new_report_path(@project.id), class: 'btn btn-success' %>
      </div>
      <hr>
      <%= render partial: 'flowchart' %>
      <%= render 'new_rule_modal' %>
      <%= render 'add_job_modal' %>
    </div>
  </div>
</div>
